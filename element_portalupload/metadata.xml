<?xml version="1.0" encoding="UTF-8"?>
<package
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://apidocs.kajona.de/xsd/package.xsd"
        >
    <title>portalupload</title>
    <description>Provides a way to allow users to upload their own files into the mediamanager via the portal, so without accessing the backend.</description>
    <version>3.4.9</version>
    <author>Kajona Team</author>
    <target>element_portalupload</target>
    <type>ELEMENT</type>
    <providesInstaller>TRUE</providesInstaller>
    <requiredModules>
        <module name="system" version="3.4.9.3" />
        <module name="pages" version="3.4.9.1" />
        <module name="mediamanager" version="3.4.9" />
    </requiredModules>
</package>
<news_list>
<div class="newsList">
    <div class="newsListHeader">
        <div class="newsListTitle">
           <h3>%%news_start_date%% - <a href="javascript:fold('cont_%%news_id%%');">%%news_title%%</a></h3>
        </div>
        <div class="newsListMore">%%news_more_link%%</div>
        <div class="cleaner"></div>
    </div>
    <div class="newsListTeaser">
        <div id="cont_%%news_id%%" style="display: none;">
            <div>%%news_intro%%</div>
        </div>
    </div>
</div>
</news_list>


<news_detail>
<div class="newsDetail">
    <h3>%%news_title%%</h3>
    <p class="newsTeaser">%%news_intro%%</p>
    %%news_image%%
    <p>%%news_text%%</p>
    <p>%%news_back_link%%</p>
</div>
</news_detail>

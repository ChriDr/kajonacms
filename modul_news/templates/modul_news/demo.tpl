<news_list>
<div class="newsList">
    <div class="newsListHeader">
        <div class="newsListTitle">
           <h3>%%news_start_date%% - %%news_title%%</h3>
        </div>
        <div class="newsListMore">%%news_more_link%%</div>
        <div class="cleaner"></div>
    </div>
    <div class="newsListTeaser">
        <div>%%news_intro%%</div>
    </div>
</div>
</news_list>

<news_list_wrapper>
    %%news%%
    <div>
        <table border="0">
            <tr>
                <td align="left">%%link_back%%</td>
                <td align="center">%%link_pages%%</td>
                <td align="right">%%link_forward%%</td>
            </tr>
        </table>
    </div>
</news_list_wrapper>


<news_detail>
<div class="newsDetail">
    <h3>%%news_title%%</h3>
    <p class="newsTeaser">%%news_intro%%</p>
    %%news_image%%
    <p>%%news_text%%</p>
    <p>%%news_back_link%%</p>
</div>
</news_detail>

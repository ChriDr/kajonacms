<faqs_list>
<div class="faqsList">
    <br />%%faq_categories%%
</div>
</faqs_list>


<faq_category>
    <div class="faqCategory">
        <div class="faqCategoryTitle"><h3>%%faq_cat_title%%</h3></div>
        %%faq_faqs%%
    </div>
</faq_category>

<faq_faq>
    <div class="faqFaq">
        <div class="faqFaqQuestion">
            <table cellpadding="0" cellspacing="0">
                <tr>
                    <td><img src="_webpath_/portal/pics/kajona/icon_question.gif" /></td>
                    <td style="padding-left: 2px;"><a href="javascript:toggle('%%faq_systemid%%');">%%faq_question%%</a></td>
                </tr>
            </table>
        </div>
        <div class="faqFaqAnswer" id="%%faq_systemid%%" style="display: none;"><a name="%%faq_systemid%%"></a>%%faq_answer%%</div>
    </div>
</faq_faq>

